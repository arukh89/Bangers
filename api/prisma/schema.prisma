generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Leader {
  id    Int    @id @default(autoincrement())
  name  String @unique
  score Int
}
model Member {
  fid         Int     @id
  username    String
  displayName String
  pfp         String?
}
model Like {
  id     Int    @id @default(autoincrement())
  fid    Int
  castId String
  @@unique([fid, castId])
}
